<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Models PDF</title>
    <style>
        body { font-family: Arial, sans-serif; }
    </style>
</head>
<body>
    <h1>List of Models</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Type</th>
                <th>Material</th>
                <th>Length</th>
                <th>Comment</th>
                <th>Publish</th>
            </tr>
        </thead>
        <tbody>
            {% for model in entities %}
            <tr>
                <td>{{ model.name }}</td>
                <td>{{ model.type }}</td>
                <td>{{ model.material }}</td>
                <td>{{ model.length }}</td>
                <td>{{ model.comment }}</td>
                <td>{{ model.publish ? 'Yes' : 'No' }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</body>
</html>
